def removeDuplicates(self, nums: List[int]) -> int:
        if not nums:
            return 0
        l=[]
        l.append(nums[0])
        for i in range(1,len(nums)):
            if nums[i-1]!=nums[i]:
                l.append(nums[i])
        for i in range(len(l)):
            nums[i]=l[i]
        return len(l)
